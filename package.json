{
  "name": "cloud-termux",
  "version": "1.2.0",
  "description": "Plataforma completa de nuvem pessoal inspirada no Nextcloud - Mobile-First com sistema adaptativo",
  "main": "server-adaptive.js",
  "scripts": {
    "start": "node server-adaptive.js",
    "dev": "nodemon server-adaptive.js",
    "safe-mode": "cp package-safe.json package.json && npm install --no-optional --production",
    "install-termux": "chmod +x install-termux.sh && ./install-termux.sh",
    "fix": "chmod +x fix-cloud-termux.sh && ./fix-cloud-termux.sh"
  },
  "keywords": ["nextcloud", "termux", "cloud", "file-sharing", "collaboration", "self-hosted"],
  "author": "Deivid Apps",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "bcryptjs": "^2.4.3",
    "socket.io": "^4.7.5",
    "express-fileupload": "^1.5.0",
    "mime": "^4.0.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.2"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.6.0",
    "sql.js": "^1.10.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "termux": {
    "optimized": true,
    "mobile_interface": true,
    "adaptive_database": true,
    "fallback_storage": "json",
    "build_tools_required": false
  }
}